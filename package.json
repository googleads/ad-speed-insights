    "chai": "^4.1.2",
    "eslint": "^5.15.3",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^5.1.1",
    "sinon": "^5.0.7",
    "typescript": "^2.9.1"
  }
}
